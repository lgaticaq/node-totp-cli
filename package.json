{
  "name": "totp-cli",
  "version": "2.0.0",
  "description": "Insert/Generate a TOTP for a service from pass",
  "main": "src",
  "scripts": {
    "prepublish": "eslint src"
  },
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "totp": "src/totp.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/node-totp-cli.git"
  },
  "keywords": [
    "totp",
    "pass",
    "cli"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/node-totp-cli/issues"
  },
  "homepage": "https://github.com/lgaticaq/node-totp-cli#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "copy-paste": "^1.3.0",
    "inquirer": "^1.1.2",
    "notp": "^2.0.3",
    "thirty-two": "^1.0.2",
    "update-notifier": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^3.2.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        2,
        2
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-console": [
        0
      ]
    }
  }
}
