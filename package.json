{
  "name": "totp-cli",
  "version": "1.0.0",
  "description": "Insert/Generate a TOTP for a service from pass",
  "main": "lib",
  "scripts": {
    "prepublish": "npm run build -s",
    "prebuild": "npm run lint -s && npm run clean -s",
    "build": "babel src --out-dir lib --source-maps",
    "lint": "eslint src",
    "clean": "rimraf lib"
  },
  "engines": {
    "node": ">=0.12"
  },
  "bin": {
    "totp": "lib/totp.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/node-totp-cli.git"
  },
  "keywords": [
    "totp",
    "pass",
    "cli"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/node-totp-cli/issues"
  },
  "homepage": "https://github.com/lgaticaq/node-totp-cli#readme",
  "dependencies": {
    "commander": "^2.9.0",
    "copy-paste": "^1.1.4",
    "inquirer": "^0.11.1",
    "notp": "^2.0.3",
    "thirty-two": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^2.9.0",
    "rimraf": "^2.5.0"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-console": [
        0
      ]
    },
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
